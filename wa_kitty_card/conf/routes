
GET     /listEvents                           controllers.UiController.index()

GET    /playersState                          controllers.UiController.playersState()

GET    /gridColors                            controllers.UiController.gridColors()

GET    /combinedView                          controllers.UiController.combinedView()

GET    /                                      controllers.UiController.HelloKitty()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file                   controllers.Assets.versioned(path="/public", file: Asset)
